{
  "name": "@brandocms/europacss",
  "version": "0.14.0",
  "description": "Europa CSS system",
  "main": "lib/index.js",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "helpers"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest __tests__ --coverage",
    "prebuild": "rimraf lib",
    "build": "swc src --out-dir lib --copy-files"
  },
  "author": "Univers <mail@univers.agency>",
  "license": "MIT",
  "repository": "https://github.com/brandocms/europacss.git",
  "bugs": {
    "url": "https://github.com/brandocms/europacss/issues"
  },
  "homepage": "https://github.com/brandocms/europacss",
  "files": [
    "src/*",
    "lib/*",
    "stubs/*",
    "*.css",
    "*.js"
  ],
  "dependencies": {
    "@outstand/postcss-extend-rule": "^4.1.2",
    "css-mqgroup": "^7.1.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-extend-rule": "^4.0.0",
    "postcss-functions": "^4.0.0",
    "postcss-nested": "7.0.2",
    "postcss-nesting": "^13.0.1",
    "postcss-reporter": "^7.1.0",
    "reduce-css-calc": "^2.1.8"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.55",
    "@swc/jest": "0.2.26",
    "@swc/register": "0.1.10",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "lightningcss": "^1.29.2",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "postcss": "^8.0.5"
  },
  "jest": {
    "testTimeout": 30000,
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.test\\.skip\\.js$",
      "<rootDir>/__tests__/fixtures/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!lightningcss)"
    ],
    "transform": {
      "\\.js$": "@swc/jest",
      "\\.ts$": "@swc/jest"
    },
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "not edge <= 18",
    "not ie 11",
    "not op_mini all"
  ]
}
